{
    "name": "@djream/core",
    "version": "0.1.2",
    "description": "Djream is a framework for building React frontends for Django projects.",
    "license": "BSD",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "build": "tsup src/index.tsx --format cjs --dts --minify --clean --sourcemap --out-dir dist",
        "check-types": "tsc --noEmit"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "immer": "^9.0.12"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@types/node": "^12.0.0",
        "@types/react": "18",
        "@types/react-dom": "^18.0.6",
        "react": "^16.13.0",
        "react-dom": "^16.13.0",
        "tsup": "^8.0.1",
        "typescript": "^4.5.5"
    },
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
        "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
    }
}
